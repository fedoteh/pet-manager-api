{
  "name": "pet-manager-api",
  "version": "1.0.0",
  "main": "dist/app.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "test": "jest --detectOpenHandles",
    "dev": "nodemon -L src/app.ts",
    "build": "tsc -p . && npm run sentry:sourcemaps",
    "start": "node -r module-alias/register dist/app.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org federico-benavidez --project pet-manager-api ./dist && sentry-cli sourcemaps upload --org federico-benavidez --project pet-manager-api ./dist"
  },
  "keywords": [
    "pet",
    "manager"
  ],
  "author": "Federico Benavidez",
  "license": "MIT",
  "description": "An application for managing pets",
  "dependencies": {
    "@sentry/cli": "^2.37.0",
    "@sentry/node": "^8.34.0",
    "@sentry/profiling-node": "^8.34.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "module-alias": "^2.2.3",
    "pg": "^8.13.0",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.4",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "_moduleAliases": {
    "@util": "dist/util",
    "@routes/v1": "dist/routes/v1",
    "@db_mock": "dist/db_mock",
    "@controllers": "dist/controllers"
  }
}
